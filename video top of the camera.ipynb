   "source": [
    "import cv2\n",
    "cam = cv2.VideoCapture(0)\n",
    "while True:\n",
    "    status , photo = cam.read()\n",
    "    photo[0:200,0:150] = photo[100:300,150:300]\n",
    "    cv2.imshow(\"myphoto\",photo)\n",
    "    if cv2.waitKey(10) == 13:\n",
    "        break\n",
    "cv2.destroyAllWindows()\n",
    "cam.release()"
   ]
